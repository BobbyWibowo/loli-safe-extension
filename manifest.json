{
  "manifest_version": 2,
  "name": "lolisafe Uploader",
  "description": "Uploader for lolisafe",
  "version": "2.0.0",
  "options_ui": {
    "browser_style": true,
    "chrome_style": true,
    "page": "options.html"
  },
  "icons": {
    "48": "logo-48x48.png",
    "128": "logo-128x128.png"
  },
  "permissions": [
    "<all_urls>",
    "clipboardWrite",
    "menus",
    "notifications",
    "storage",
    "tabs",
    "webRequest",
    "webRequestBlocking"
  ],
  "background": {
    "scripts": [
      "axios.min.js",
      "background.js"
    ]
  },
  "applications": {
    "gecko": {
      "id": "lolisafe-extension@BobbyWibowoB.addons.mozilla.org"
    }
  }
}
